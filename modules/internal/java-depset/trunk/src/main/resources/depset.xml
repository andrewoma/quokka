<dependency-set>
    <profile id="skiptest" description="Skips tests, disabling all test-related targets"/>

    <dependency-set group="quokka.depset.jar" version="0.3-ss"/>
    <dependency-set group="quokka.internal.module-depset" version="0.3-ss"/>

    <plugin group="quokka.plugin.junit" version="0.3-ss" profiles="!skiptest"/>

    <plugin group="quokka.plugin.junitreport" version="0.3-ss" use-defaults="false" profiles="!skiptest">
        <target name="report" profiles="!hasParent"/>
        <target name="collect" profiles="hasParent"/>
    </plugin>

    <plugin group="quokka.plugin.cobertura" version="0.3-ss" use-defaults="false" profiles="!skiptest">
        <target name="report" profiles="!hasParent"/>
        <target name="collect" profiles="hasParent"/>
    </plugin>

    <plugin group="quokka.plugin.help" version="0.3-ss" use-defaults="false">
        <target name="generate-help"/>
    </plugin>

    <plugin group="quokka.plugin.jalopy" version="0.3-ss"/>
    <plugin group="quokka.plugin.source" version="0.1-ss"/>
    <plugin group="quokka.plugin.javadoc" version="0.3-ss" use-defaults="false" targets="collect"/>
</dependency-set>
