<dependency-set>
    <!-- Define the path for integration tests -->
    <path id="quokka-itest" description="Path for running quokka integration tests"/>
    <dependency group="apache.ant" version="1.7.0" paths="quokka-itest"/>
    <dependency group="apache.ant" name="ant-launcher" version="1.7.0" paths="quokka-itest"/>
    <!--<dependency group="apache.xerces" version="2.6.2" paths="quokka-itest"/>-->
    <!--<dependency group="apache.xerces" name="xml-apis" version="2.6.2" paths="quokka-itest"/>-->
    <dependency group="quokka.core.main" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.metadata" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.plugin-spi" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.repo-spi" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.repo-standard" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.util" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.model" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.bootstrap" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.bootstrap-util" version="1.0-m01-ss" paths="quokka-itest"/>
    <dependency group="quokka.core.version" version="1.0-m01-ss" paths="quokka-itest"/>

    <!-- Integration test -->
    <dependency group="quokka.core.itest" version="1.0-m01-ss" paths="itest,quokka-itest+"/>

    <!--<plugin group="quokka.plugin.plugin" version="1.0-m01-ss"/>-->
    <dependency-set group="quokka.internal.module-depset" version="1.0-m01-ss"/>
</dependency-set>
