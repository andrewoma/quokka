<dependency-set>
    <dependency-set group="quokka.internal.module-depset" version="0.3"/>

    <property name="q.resources.main.output"
              value="${q.lifecycle.resourcesOutput}/META-INF/quokka/${q.project.artifact.group}_${q.project.artifact.name[depset]}_depset_${q.project.artifact.version}"/>

    <plugin group="quokka.plugin.resources" version="0.3"/>
    <plugin group="quokka.plugin.standard-lifecycle" version="0.3"/>
    <plugin group="quokka.plugin.jar" version="0.3"/>

    <plugin group="quokka.plugin.help" version="0.3" use-defaults="false">
        <target name="generate-help"/>
    </plugin>
</dependency-set>
