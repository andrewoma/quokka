<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project PUBLIC "quokka.ws/dtd/project-0.1" "http://quokka.ws/dtd/project-0.1.dtd">

<project>
    <description>Quokka bootstrapping module: allows automatic booting of different quokka versions</description>

    <artifacts group="quokka.core.bootstrap" version="0.3-ss">
        <artifact paths="runtime"/>
    </artifacts>

    <bootstrap core="[0.2.1,0.2.1]" spec-version="[1.4,1.6]"/>

    <dependency-set>
        <dependency group="apache.ant" version="1.7.1" paths="compile, runtime"/>
        <dependency group="apache.ant" name="launcher" version="1.7.1" paths="compile, runtime"/>
        <dependency group="quokka.core.bootstrap-util" version="0.3-ss" paths="compile, runtime"/>
        <dependency group="quokka.core.version" version="0.3-ss" paths="compile, runtime"/>
        <dependency group="quokka.core.test" version="0.3-ss" paths="test-compile(junit)"/>

        <dependency-set group="quokka.internal.java-depset" version="0.1"/>
    </dependency-set>
</project>
