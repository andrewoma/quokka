<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project PUBLIC "quokka.ws/dtd/project-0.2" "http://quokka.ws/dtd/project-0.2.dtd">

<project>
    <description>Quokka integration test module: provides base classes for integration tests</description>

    <artifacts group="quokka.core.itest" version="0.3-ss">
        <artifact paths="runtime"/>
    </artifacts>

    <bootstrap core="[0.3-ss,0.3-ss]" spec-version="[1.4,1.6]"/>

    <dependency-set>
        <dependency group="quokka.core.test" version="0.3-ss" paths="compile(junit), runtime"/>
        <dependency group="apache.ant" version="1.7.1" paths="compile"/>
        <dependency group="quokka.core.repo-spi" version="0.3-ss" paths="compile"/>
        <dependency group="quokka.core.bootstrap-util" version="0.3-ss" paths="compile"/>
        <dependency group="quokka.core.util" version="0.3-ss" paths="compile"/>
        <dependency group="quokka.core.version" version="0.3-ss" paths="compile"/>
        <dependency group="quokka.core.plugin-spi" version="0.3-ss" paths="compile, runtime"/>

        <dependency-set group="quokka.internal.java-depset" version="0.2-ss"/>
    </dependency-set>
</project>
